msgid "Display version information"
msgstr "Exibe informação da versão"

#: ../xfce4-notifyd-config/main.c:886
msgid "Settings manager socket"
msgstr "Socket do gerenciador de configurações"

#: ../xfce4-notifyd-config/main.c:886
msgid "SOCKET_ID"
msgstr "SOCKET_ID"

